<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:rm="CEN/13606/RM" xmlns="http://eHealth.gov.sk/MPD/v1" targetNamespace="http://eHealth.gov.sk/MPD/v1" elementFormDefault="qualified">
  <xs:import namespace="CEN/13606/RM" schemaLocation="../../COMMON/TS14796-dataTypes.xsd" />
  <xs:include schemaLocation="../_shared/MPD_IdCiselniky.xsd" />
  <xs:include schemaLocation="../_shared/MPD_Identifikatory.xsd" />
  <xs:include schemaLocation="../_shared/ZdravotnickyPracovnikOU.xsd" />
  <xs:include schemaLocation="../_shared/DietetickaPotravinaDetail.xsd" />
  <xs:element name="DajDetailReceptuVystup" type="DajDetailReceptuVystup" />
  <xs:complexType name="DajDetailReceptuVystup">
    <xs:annotation>
      <xs:documentation>Detailné údaje eReceptu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DatumVystavenia" type="xs:date" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Dátum vystavenia eReceptu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="HradiPacient" type="xs:boolean" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či hradí pacient alebo poisťovňa.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdReceptu" type="rm:II" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikátor eReceptu (OID, ext).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdZaznamu" type="xs:long" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>RDBMS-Z ID receptu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IndBlok" type="xs:boolean" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či je eRecept blokovaný.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IndStorno" type="xs:boolean" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či je eRecept stornovaný.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IndViacnasobnyRecept" type="xs:boolean" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či ide o viacnásobný eRecept.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IndZneplatnenia" type="xs:boolean" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či je eRecept zneplatnený.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NeodkladnaZS" type="xs:boolean" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či ide o predpis s poznámkou "Neodkladná zdravotná starostlivosť potvrdená zdravotnou poisťovňou".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NezmluvnyLekar" type="xs:boolean" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či predpisoval nezmluvný lekár.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OsobitnyPredpis" type="xs:boolean" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či ide o osobitný predpis.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Perioda" type="xs:int" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Udáva sa pre viacnásobné recepty. Určuje periódu opakovaného výdaja z tohto receptu v mesiacoch. Napr. 3 znamená, že môžem vybrať každé 3 mesiace.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PlatnostDo" type="xs:date" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Vypočítaný koniec platnosti lekárskeho predpisu alebo poukazu. Tento atribút označuje posledný deň, keď je ešte eRecept platný.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PlatnostOd" type="xs:date" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Vo väčšine prípadov zhodná sdátumom vystavenia. Odlišuje sa ak bol recept vystavený dodatočne, napr. neodkladná ZS.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PocetOpakovani" type="xs:int" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak je 1, tak ide o jednorázový recept. Ak je viac ako 1, tak koľko krát sa dá vybrať na tento recept.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Poistovna" type="IdZP" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikátor zdravotnej poisťovne.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Povod" type="ElektronickyReceptPovod" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Určenie pôvodu receptu, podľa služby ktorá recept zaznamenala v eHealth.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PrZS" type="IdPrZS" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Šifrovaný identifikátor príjmateľa zdravotnej starostlivosti.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="VynutenyZapis" type="xs:boolean" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či bol eRecept zapísaný s príznakom "vynútený zápis".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="VynutenyZapisDovod" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Dôvod vynúteného zápisu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Blokovanie" type="Blokovanie" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Údaje blokovania eReceptu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Zneplatnenie" type="Zneplatnenie" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Údaje zneplatnenia eReceptu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ZastupovanyZPrac" type="ZdravotnickyPracovnikOU" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikačné údaje zdravotníckeho pracovníka, ktorý je na eRecepte evidovaný ako zastupujúci zdravotnícky pracovník.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Predpisal" type="ZdravotnickyPracovnikOU" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikačné údaje zdravotníckeho pracovníka, ktorý eRecept predpísal.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Storno" type="Storno" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Údaje storna eReceptu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DispenzacnyZaznam" type="DetailReceptuDispenzacnyZaznam" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Dispenzačný záznam eReceptu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PreskripcnyZaznam" type="DetailReceptuPreskripcnyZaznam" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Preskripčný záznam eReceptu.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DetailReceptuDispenzacnyZaznam">
    <xs:annotation>
      <xs:documentation>Detailné údaje dispenzačného záznamu spoločné pre všetky typy dispenzačných záznamov. Pre jednotlivé typy dispenzačných liečebných prostriedkov (liek, IPL, pomôcky, potraviny) sú evidované aj doplňujúce údaje špecifické pre daný typ liečebného prostriedku. Podľa typu použitého liečebného prostriedku je vyplnená práve jedna špecifická časť.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DatumVydaja" type="xs:date" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Dátum výdaja lieku, dietetickej potraviny alebo zdravotníckej pomôcky.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Diagnoza" type="IdPolozkaCiselnikaDiagnoza" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>CV identifikátor (vrátane displayName) diagnózy.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DoplatokPoistenca" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Výška úhrady poistenca za liečebný prostriedok.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DoplatokPredZlavou" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Doplatok poistenca pred zľavou.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdDispenzacnehoZaznamu" type="rm:II" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikátor dispenzačného záznamu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdMedikacnehoZaznamu" type="rm:II" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikátor medikačného záznamu, ktorý vznikol k tomuto dispenzačnému záznamu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdPreskripcnehoZaznamu" type="rm:II" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikátor preskripčného záznamu, na základe ktorého bol dispenzačný záznam vytvorený.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdSplnomocnenia" type="xs:long" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak bol liečebný prostriedok vydaný na základe splnomocnenia, tak tu je uvedený jeho identifikátor v EZK.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdZaznamu" type="xs:long" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>RDBMS-Z ID dispenzačného záznamu</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PocetDniUzivania" type="xs:int" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak lekárnik doplní počet dní užívania.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PoplatokZaVydaj" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Poplatok za služby hradení poistencom. Hodnoty: P / O / Z.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Postovne" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Úhrada za poštovné. Pre zdravotnícke pomôcky.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Prevzal" type="IdPrZS" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Šifrovaný identifikátor prijímateľa ZS, ktorý prebral liek. Slúži hlavne na evidenciu toho kto vyzdvihol liek s omamnou alebo psychotropnou látkou a na zaevidovanie osoby, ktorá vyzdvihla liečebný prostriedok na základe splnomocnenia.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SpravaLekarnika" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Napr. správa lekárnika, iná poznámka, upozornenie od lekárnika a podobne.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UhradaPredZlavou" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Úhrada poisťovne pred zľavou.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UhradaZoZP" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Časť ceny vydaného lieku alebo zdravotníckej pomôcky, ktorú uhrádza príslušná zdravotná poisťovňa.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UpresnenieDiagnozy" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Textové upresnenie diagnózy.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="VynutenyZapis" type="xs:boolean" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Informácia, či bol dispenzačný záznam zapísaný s príznakom "vynútený zápis".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="VynutenyZapisDovod" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Dôvod vynúteného zápisu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ZdravotnaPoistovna" type="IdZP" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikátor zdravotnej poisťovne.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Vydal" type="ZdravotnickyPracovnikOU" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikačné údaje zdravotníckeho pracovníka, ktorý liečebný prostriedok vydal.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Storno" type="Storno" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Údaje storna dispenzačného záznamu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ZdravotnickaPomocka" type="DispenzacnyZaznamZdravotnickaPomocka" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak sa jedná o dispenzačný záznam zdravotníckej pomôcky, tak v tejto časti sa vracajú detailné informácie o výdaji danej pomôcky a informácie o nej.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DietetickaPotravina" type="DispenzacnyZaznamDietetickaPotravina" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak sa jedná o dispenzačný záznam dietetickej potraviny, tak v tejto časti sa vracajú detailné informácie o výdaji dietetickej potraviny a informácie o nej.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IPL" type="DispenzacnyZaznamIPL" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak sa jedná o dispenzačný záznam individuálne pripraveného lieku, tak v tejto časti sa vracajú detailné informácie o výdaji daného lieku a informácie o tomto lieku.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Liek" type="DispenzacnyZaznamLiek" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak sa jedná o dispenzačný záznam lieku, tak v tejto časti sa vracajú detailné informácie o výdaji daného lieku a informácie o tomto lieku.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DetailReceptuPreskripcnyZaznam">
    <xs:annotation>
      <xs:documentation>Detailné údaje preskripčného záznamu spoločné pre všetky typy preskripčných záznamov. Pre jednotlivé typy preskripčných liečebných prostriedkov (liek, IPL, pomôcky, potraviny) sú evidované aj doplňujúce údaje špecifické pre daný typ liečebného prostriedku. Podľa typu použitého liečebného prostriedku je vyplnená práve jedna špecifická časť.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Diagnoza" type="IdPolozkaCiselnikaDiagnoza" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>CV identifikátor (vrátane displayName) diagnózy.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdPreskripcnehoZaznamu" type="rm:II" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikátor preskripčného záznamu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MnozstvoOmamnejLatky" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Množstvo omamnej alebo psychotropnej látky v jednom balení humánneho lieku vyjadrené v hmotnostných, objemových alebo kusových jednotkách.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MnozstvoOmamnejLatkyLatinsky" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Množstvo omamnej alebo psychotropnej látky v jednej dávke slovom po latinsky.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Nazov" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Názov predpísaného lieku, alebo liečiva, alebo IPL, alebo dietetickej potraviny, alebo zdravotníckej pomôcky.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PocetDispenzacii" type="xs:int" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Počet dispenzácií preskripčného záznamu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SpravaLekara" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Napr.správa lekára lekárnikovi, zdvôvodnenie preskripcie napriek upozornenie znalostného systému.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SuhlasPoistovneCisloRozhodnutia" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Číslo rozhodnutia, ak bola preskripcia dopredu schválená revíznym lekárom.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SuhlasPoistovneDatum" type="xs:date" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Dátum schválenia, ak bola preskripcia dopredu schválená revíznym lekárom.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SuhlasPoistovnePoznamka" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ľubovoľná poznámka k súhlasu poisťovne, ak bola preskripcia dopredu schválená revíznym lekárom.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UpresnenieDiagnozy" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Textové upresnenie diagnózy.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ZaciatokMedikacie" type="xs:date" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Dátum od ktorého sa má liečebný prostriedok užívať. Ak nie je zadaný, tak v oboch prípadoch predpokladáme, že je zhodný s dátumom dispenzácie.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Odporucanie" type="Odporucanie" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Údaje odporúčania, na základe ktorého bol predpísaný alebo podaný daný liečebný prostriedok.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ReviznyLekar" type="ZdravotnickyPracovnikOU" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikačné údaje zdravotníckeho pracovníka, ktorý je na preskripčnom zázname evidovaný ako revízny lekár.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Storno" type="Storno" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Údaje storna preskripčného záznamu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Liek" type="PreskripcnyZaznamLiek" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak sa jedná o preskripčný záznam lieku, tak v tejto časti sa vracajú detailné informácie o predpísaní daného lieku a informácie o tomto lieku.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Liecivo" type="PreskripcnyZaznamLiecivo" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak sa jedná o preskripčný záznam liečiva, tak v tejto časti sa vracajú detailné informácie o predpísaní daného liečiva a informácie o ňom.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IPL" type="PreskripcnyZaznamIPL" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Ak sa jedná o preskripčný záznam individuálne pripraveného lieku, tak v tejto časti sa vracajú detailné informácie o predpísaní daného lieku a informácie o tomto lieku.</xs:documentation>
        </xs:annotation>
      </xs:element>
