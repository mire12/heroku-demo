    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratornaVzorka">
    <xs:sequence>
      <xs:element name="AnatomickeMiestoOdberu" type="ACV" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
      		<xs:documentation>Anatomické miesto, odkiaľ je vzorka odobratá.
Číselník obsahujúci zoznam anatomických pojmov pre potreby identifikácie konkrétneho anatomického miesta.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DatumOdberu" type="xs:dateTime" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
          <xs:documentation>Dátum a čas, kedy bol vykonaný odber </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DatumPrijatia" type="xs:dateTime" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
          <xs:documentation>Dátum a čas prijatia laboratórnej vzorky do laboratória</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ExternyIdentifikator" type="xs:string" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
          <xs:documentation>Externý identifikátor vzorky, ktorý slúži na komunikáciu ambulancie a laboratória </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Identifikator" type="xs:string" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
      		<xs:documentation>ID vzorky
Identifikátor laboratórnej vzorky. Musí byť zabezpečená jeho jednoznačnosť v celom systéme NZIS. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MnozstvoOdobratejVzorky" type="Mnozstvo" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
          <xs:documentation>Množstvo odobratej vzorky pre účel laboratórneho vyšetrenia</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OkolnostiOdberu" type="xs:string" minOccurs="0" maxOccurs="1">
      	<xs:annotation>
          <xs:documentation>Všetky podmienky, ktoré musia byť splnené pred odberom vzorky napr. plný močový mechúr, nalačno alebo špeciálne pokyny pre zaobchádzanie alebo okamžité spracovanie vzorky. Môže byť tiež použitý pre dokumentáciu všetkých známych odchýlok spracovania napr. pacient nebol nalačno.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Poznamka" type="xs:string" minOccurs="0" maxOccurs="1">
      	<xs:annotation>
          <xs:documentation>Poznámka k vzorke</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Riziko" type="xs:string" minOccurs="0" maxOccurs="1">
      	<xs:annotation>
          <xs:documentation>Potenciálne riziko alebo biohazard spojený so zberom a spracovaním vzorky</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdentifikaciaOUPZS" type="AIIIdentifikaciaOUPZS" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
      		<xs:documentation>Pracovník odoberajúci vzorku - Identifikácia OÚ PZS
Identifikácia odborného útvaru poskytovateľa zdravotnej starostlivosti z JRÚZ, do ktorého zdravotnícky pracovník patrí.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdentifikaciaZPrac" type="AIIIdentifikaciaZPrac" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
      		<xs:documentation>Pracovník odoberajúci vzorku - Identifikácia ZPrac
Identifikácia zdravotníckeho pracovníka v JRÚZ.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NezmluvnyLekar" type="xs:boolean" minOccurs="0" maxOccurs="1">
      	<xs:annotation>
      		<xs:documentation>Pracovník odoberajúci vzorku - Nezmluvný lekár
Doplňujúci atribút, ktorý sa použije na prenos informácie, že daný zdravotnícky pracovník - lekár, nemal pri svojom úkone zmluvný vzťah k poisťovni pacienta a teda, že tento úkon si bude hradiť pacient. Použije sa napríklad pri predpisovaní liekov.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SpecializaciaLekara" type="ACV" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
      		<xs:documentation>Pracovník odoberajúci vzorku - Špecializácia lekára
Šprecializácia tohto lekára, pod ktoru prebehla operácia, ktorú daný lekár vykonával.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SposobOdberu" type="ACV" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
      		<xs:documentation>Popis, ako bola vzorka odobratá. Napr. Venózny odber, biopsia. resekcia ...
Odkaz na čísleník SposobOdberuVzorky</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Transport" type="xs:string" minOccurs="0" maxOccurs="1">
      	<xs:annotation>
          <xs:documentation>Požiadavky na konzervačné látky alebo dopravný prostriedok pre manipuláciu so vzorkou.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransportnyObal" type="ACV" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
      		<xs:documentation>Číselník typov transportných obalov pre transport vzorky do laboratória.
Odkaz na číslenik TransportnyObal</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TypVzorky" type="ACV" minOccurs="0" maxOccurs="1" >
      	<xs:annotation>
      		<xs:documentation>Čiselníková hodnota typu vzorky.
Číselník definuje typ vzorky pre laboratórne vyšetrenie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="KlinickeUdajePacienta" minOccurs="0" maxOccurs="1">
                <xs:annotation>
          <xs:documentation>Klinické údaje pacienta evidované pri odbere vzorky na laboratórne vyšetrenie.</xs:documentation>
        </xs:annotation>
                <xs:complexType>
          <xs:sequence>
