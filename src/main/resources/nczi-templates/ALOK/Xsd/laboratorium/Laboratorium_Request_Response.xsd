<?xml version="1.0" encoding="utf-8"?>
  <xs:schema targetNamespace="http://eHealth.gov.sk/Allocation/v1"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://eHealth.gov.sk/Allocation/v1"
	elementFormDefault="qualified"
	>

  <xs:include schemaLocation="../_shared/AlokacieIdentifikatory.xsd" />
  <xs:include schemaLocation="../_shared/Obecne.xsd" />
  <xs:include schemaLocation="../_shared/ObecneAnon.xsd" />
  <xs:include schemaLocation="../_shared/Anonymizacia_Request_Response.xsd" />
  <xs:include schemaLocation="../_shared/EhrFields.xsd" />

  <xs:include schemaLocation="../_shared/Identifikatory.xsd" />

  <!-- Request -->
  <xs:element name="LaboratornyVysledokIdentifikator" type="LaboratornyVysledokIdentifikator"/>
  <xs:element name="LaboratornyVysledokIdentifikator_v2" type="LaboratornyVysledokIdentifikator_v2" />
  <xs:element name="ZiadankaIdentifikator" type="ZiadankaIdentifikator"/>
  <xs:element name="OstatneVysetrenieCriteria" type="OstatneVysetrenieCriteria" />
  <xs:element name="LaboratornaZiadankaIdentifikator" type="LaboratornaZiadankaIdentifikator"/>
  <xs:element name="LaboratorneVysetreniaCriteria" type="LaboratorneVysetreniaCriteria" />

  <xs:element name="LaboratorneZiadankyLaboratoriaCriteria" type="LaboratorneZiadankyLaboratoriaCriteria"/>
  <xs:element name="LaboratorneZiadankyPacientaCriteria" type="LaboratorneZiadankyPacientaCriteria"/>
  <xs:element name="LaboratorneVysledkyCriteria" type="LaboratorneVysledkyCriteria"/>
  <xs:element name="LaboratorneVysledkyCriteria_v2" type="LaboratorneVysledkyCriteria_v2" />
  <xs:element name="LaboratornePolozkyPreZapis" type="LaboratornePolozkyPreZapis" />
  <xs:element name="VyhladajLaboratoriaCriteria" type="VyhladajLaboratoriaCriteria"/>
  <xs:element name="LaboratornaZiadankaStav" type="LaboratornaZiadankaStav" />
  <xs:element name="ZoznamLaboratorneVysledky" type="ZoznamLaboratorneVysledky" />
  <xs:element name="DajLaboratorneVysetreniePreZiadatela_Request" type="DajLaboratorneVysetreniePreZiadatela_Request"/>
  <xs:element name="PrevezmiLaboratornuZiadanku_Response" type="xs:string" />
  <xs:element name="ZapisLaboratornePolozkyLaboratoria_Response" type="xs:string" />
  <xs:element name="ZrusLaboratornuZiadanku_Response" type="xs:string" />
  <xs:element name="LaboratornyVysledokId" type="LaboratornyVysledokId"/>
  <xs:element name="LaboratorneZiadankyZoznam" type="LaboratorneZiadankyZoznam" />
  <xs:element name="LaboratornyVysledokIdentifikatory" type="LaboratornyVysledokIdentifikatory" />
  <!-- Request end -->

  <xs:complexType name="DajLaboratorneVysetreniePreZiadatela_Request">
    <xs:sequence>
      <xs:element name="InternyIdentifikatorVzorky" type="xs:string" minOccurs="0" maxOccurs="1"/>
      <xs:element name="IdentifikatorZiadanky" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9a-zA-Z]){21}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Pacient" type="IdPrjZS" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratornyVysledokIdentifikator">
    <xs:sequence>
      <xs:element name="Vysledok" type="IdLaboratornyVysledok" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Pacient" type="IdPrjZS" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratornyVysledokIdentifikator_v2">
    <xs:sequence>
      <xs:element name="Pacient" type="IdPrjZS" minOccurs="1" maxOccurs="1" />
      <xs:element name="Vysledok" type="IdLaboratornyVysledok" minOccurs="0" maxOccurs="1" />
      <xs:element name="Vzorka" type="IdLaboratornaVzorkaCriteria" minOccurs="0" maxOccurs="1" />
      <xs:element name="Ziadanka" type="IdLaboratornaZiadankaCriteria" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratornyVysledokId">
    <xs:sequence>
      <xs:element name="Vysledok" type="IdLaboratornyVysledok" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ZiadankaIdentifikator">
    <xs:sequence>
      <xs:element name="Ziadanka" type="IdLaboratornaZiadanka" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Pacient" type="IdPrjZS" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratornaZiadankaIdentifikator">
    <xs:sequence>
      <xs:element name="Ziadanka" type="IdLaboratornaZiadanka" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratorneZiadankyLaboratoriaCriteria">
    <xs:sequence>
      <xs:element name="DatumVytvoreniaZiadankyDo" type="xs:date" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Kontroluje sa dátum vytvorenia žiadanky</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DatumVytvoreniaZiadankyOd" type="xs:date" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Kontroluje sa dátum vytvorenia žiadanky</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ExternyIdentifikatorVzorky" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Externý identifikátor vzorky, ktorý slúži na komunikáciu ambulancie a laboratória. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9a-zA-Z])*" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="IdentifikatorVzorky" type="IdLaboratornaVzorka" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifikátor laboratórnej vzorky. Musí byť zabezpečená jeho jednoznačnosť v celom systéme NZIS. Mal by podporovať využitie systému čiarových kódov 2D/3D.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Laboratorium" type="IdOUPZS" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Odkaz na číselník OdbornyUtvarPZS</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StavyZiadanky" type="StavZiadankyEnum" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="Strankovanie_Metadata" type="Strankovanie_Metadata" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="StavZiadankyEnum">
    <xs:annotation>
      <xs:documentation>Zoznam možných stavov, v ktorých môže byť ukončené laboratórne vyšetrenie na základe žiadanky.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="NEPREVZATA">
        <xs:annotation>
          <xs:documentation>Ukončený</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PREVZATA">
        <xs:annotation>
          <xs:documentation>Prerušené spracovanie</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ZRUSENA">
        <xs:annotation>
          <xs:documentation>Dočasný, neúplný výsledok.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="LaboratorneZiadankyPacientaCriteria">
    <xs:sequence>
      <xs:element name="DatumDo" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <xs:element name="DatumOd" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Pacient" type="IdPrjZS" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Laboratorium" type="IdOUPZS" minOccurs="0" maxOccurs="1"/>
      <xs:element name="StavZiadanky" type="StavZiadankyEnum" minOccurs="1" maxOccurs="unbounded"/>
      <xs:element name="ExternyIdentifikatorVzorky" type="xs:string" minOccurs="0" maxOccurs="1"/>
      <xs:element name="IdentifikatorVzorky" type="IdLaboratornaVzorka" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Strankovanie_Metadata" type="Strankovanie_Metadata" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratorneVysledkyCriteria">
    <xs:sequence>
      <xs:element name="DatumDo" type="xs:date" minOccurs="1" maxOccurs="1"/>
      <xs:element name="DatumOd" type="xs:date" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Pacient" type="IdPrjZS" minOccurs="1" maxOccurs="1"/>
      <xs:element name="IdZaznamu" type="xs:long" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>ID zaznamu laboratorneho vysledku v databazi</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StavVysledku" type="StavVysledkuEnum" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="LaboratornaVzorka" type="IdLaboratornaVzorka" minOccurs="0" maxOccurs="1"/>
      <xs:element name="LaboratornaPolozka" type="IdPolozkaCiselnika" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Odkaz na číselník LaboratornaPolozka</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LaboratornaZiadanka" type="IdLaboratornaZiadanka" minOccurs="0" maxOccurs="1"/>
      <xs:element name="DatumOdberuVzorkyOd" type="xs:date" minOccurs="0" maxOccurs="1" />
      <xs:element name="DatumOdberuVzorkyDo" type="xs:date" minOccurs="0" maxOccurs="1" />
      <xs:element name="TypVzorky" type="IdTypVzorky" minOccurs="0" maxOccurs="1" />
      <xs:element name="Strankovanie_Metadata" type="Strankovanie_Metadata" minOccurs="1" maxOccurs="1" />
      <xs:element name="Triedenie_Metadata" type="Triedenie_LabVysetreniaPacienta" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>
  <!--VyhladajLabVysetreniaPacienta_v2-->
  <xs:complexType name="LaboratorneVysledkyCriteria_v2">
    <xs:sequence>
      <!--Maximálna požadovaná citlivosť záznamov. Default = 5-->
      <xs:element name="Citlivost" type="xs:int" minOccurs="0" maxOccurs="1" />
      <!--Dátum a čas kompletizácie a vytvorenia laboratórneho výsledku. Ak nie je vyplnený, použije sa dátum zaradenia záznamu do EHR - time_commited-->
      <xs:element name="DatumVysetreniaDo" type="xs:date" minOccurs="0" maxOccurs="1" />
      <!--Dátum a čas kompletizácie a vytvorenia laboratórneho výsledku. Ak nie je vyplnený, použije sa dátum zaradenia záznamu do EHR - time_commited-->
      <xs:element name="DatumVysetreniaOd" type="xs:date" minOccurs="0" maxOccurs="1" />
      <!--Dátum zápisu do nzis (time_created)-->
      <xs:element name="DatumZapisuDo" type="xs:date" minOccurs="0" maxOccurs="1" />
      <!--Dátum zápisu do nzis (time_created)-->
      <xs:element name="DatumZapisuOd" type="xs:date" minOccurs="0" maxOccurs="1" />
      <!-- 
      Identifikátor laboratórnej žiadanky.

      Obsahuje identifikátor a typ identifikátora
      Povolené sú nasledovné typy:
      - PZS_EX - externý identifikátor generovaný poskytovateľom ZS
      - LAB_EX - externý identifikátor generovaný laboratóriom
      - INTERNAL - interný identifikátor OID: 1.3.158.00165387.100.60.20
      -->
      <xs:element name="LaboratornaZiadankaId" type="LaboratornaZiadankaIdTyp" minOccurs="0" maxOccurs="unbounded" />
      <!--
      Identifikátor realizovaného laboratórneho testu.
      Laboratórny výsledok -> Výsledok testu -> Výsledok -> Realizovaný test
      Môže nadobúdať nasledovné typy:
      - LOINC kod podľa OID=2.16.840.1.113883.6.1
      - LOINC_JRUZ
      - LAB_KOD 
      -->
      <xs:element name="LaboratornyTestId" type="LaboratornyTestIdTyp" minOccurs="0" maxOccurs="unbounded" />
      <!--Šifrovaný identifikátor pacienta-->
      <xs:element name="Pacient" type="IdPrjZS" minOccurs="1" maxOccurs="1" />
      <!--Stav laboratórneho výseldku.-->
      <xs:element name="StavVysledku" type="StavVysledkuEnum" minOccurs="0" maxOccurs="unbounded" />
      <!--
      Príznak, či sa majú vyhľadávať vlastné záznamy alebo záznamy podľa definovaného prístupu CM. 
      Ak je nastavený na TRUE - vyhľadávajú sa len vlastné záznamy
      Ak je nastavený na FALSE - vyhľadávajú sa záznamy podľa consentu
      Ak nie je uvedený, vyhľadávajú sa vlastné záznamy aj zaznamý podľa consentu
      -->
      <xs:element name="VlastneZaznamy" type="xs:boolean" minOccurs="0" maxOccurs="1" />
      <xs:element name="Strankovanie_Metadata" type="Strankovanie_Metadata" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="StavVysledkuEnum">
    <xs:annotation>
      <xs:documentation>
        Umožňuje zadefinovať v akom stave chcem vyhľadať požadovaný výsledok.
        Je možné definovať žiadne, práve jedno alebo množinu stavov.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="FINAL">
        <xs:annotation>
          <xs:documentation>
            Ukončený
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CORRECTED">
        <xs:annotation>
          <xs:documentation>
            Oprava výsledkov
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABORTED">
        <xs:annotation>
          <xs:documentation>
            Prerušené spracovanie
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="INTERIM">
        <xs:annotation>
          <xs:documentation>
            Dočasný, neúplný výsledok.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="LaboratornyTestIdTypEnum">
    <xs:annotation>
      <xs:documentation>Zoznam povolených typov identifikátorov, ktoré môžu byť použité na identifikáciu laboratórneho testu.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="LOINC">
        <xs:annotation>
          <xs:documentation>kod podľa OID=2.16.840.1.113883.6.1</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="LOINC_JRUZ">
        <xs:annotation>
          <xs:documentation>LOINC z JRUZ ("stary")</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="LAB_KOD">
        <xs:annotation>
          <xs:documentation>kod z laboratoria</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="LaboratornaZiadankaIdTypEnum">
    <xs:annotation>
      <xs:documentation>Zoznam povolených typov identifikátorov, ktoré môžu byť použité na identifikáciu laboratórnej žiadanky.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="PZS_EX">
        <xs:annotation>
          <xs:documentation>externý identifikátor generovaný poskytovateľom ZS</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="LAB_EX">
        <xs:annotation>
          <xs:documentation>externý identifikátor generovaný laboratóriom</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="INTERNAL">
        <xs:annotation>
          <xs:documentation>interný identifikátor OID: 1.3.158.00165387.100.60.20</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="LaboratornyTestIdTyp">
    <xs:sequence>
      <xs:element name="Identifikator" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="TypIdentifikatora" type="LaboratornyTestIdTypEnum" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LaboratornaZiadankaIdTyp">
    <xs:sequence>
      <xs:element name="Identifikator" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="TypIdentifikatora" type="LaboratornaZiadankaIdTypEnum" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LaboratornaZiadankaStav">
    <xs:sequence>
      <xs:element name="StavZiadanky" type="StavZiadankyEnum" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="LaboratornaZiadankaId" type="LaboratornaZiadankaId"/>
  <xs:complexType name="LaboratornaZiadankaId">
    <xs:sequence>
      <xs:element name="IdentifikatorVzorky" type="IdLaboratornaVzorka" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Vygenerovaný identifikátor vzorky</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Ziadanka" type="IdLaboratornaZiadanka" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratorneVysetreniaCriteria">
    <xs:sequence>
      <xs:element name="DatumDo" type="xs:date" minOccurs="1" maxOccurs="1"/>
      <xs:element name="DatumOd" type="xs:date" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Poistovna" type="IdZP" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Odkaz na číselník ZdravotnaPoistovna</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Pacient" type="IdPrjZS" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Odkaz na číselník PrijimatelZS</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Strankovanie_Metadata" type="Strankovanie_Metadata" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:element name="ZoznamLaboratornychVysledkov" type="ZoznamLaboratornychVysledkov"/>
  <xs:complexType name="ZoznamLaboratornychVysledkov">
    <xs:sequence>
      <xs:element name="Pacient" type="IdPrjZS" minOccurs="1" maxOccurs="1" />
      <xs:element name="Vysetrenia" type="LaboratorneVysledkyZakladneUdaje" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratorneVysledkyZakladneUdaje">
    <xs:sequence>
      <xs:element name="LaboratornyVysledokZakladneUdaje" type="LaboratornyVysledokZakladneUdaje" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>

  <xs:element name="LaboratornyVysledokZakladneUdaje" type="LaboratornyVysledokZakladneUdaje"/>
  <xs:complexType name="LaboratornyVysledokZakladneUdaje">
    <xs:annotation>
      <xs:documentation>Laboratorne vysledok so zoznamom poloziek bez uvedenia hodnoty</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DatumOdberu" type="xs:dateTime" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Dátum a čas kompletizácie a vytvorenia laboratórneho výsledku</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Laboratorium" type="IdOUPZS" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Odkaz na číselník OdbornyUtvarPZS</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ZaznamenanyLekarom" type="IdZdrPrac" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Odkaz na číselník ZdravotnickyPracovnik</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Ziadatel" type="PracovnikOdbornehoUtvaru" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Polozky" type="Polozka" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Polozka">
    <xs:sequence>
      <xs:element name="Polozka" type="LaboratornaPolozkaBezVysledku" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>

  <xs:element name="LaboratornaPolozkaBezVysledku" type="LaboratornaPolozkaBezVysledku"/>
  <xs:complexType name="LaboratornaPolozkaBezVysledku">
    <xs:annotation>
      <xs:documentation>Laboratorne vysledok so zoznamom poloziek bez uvedenia hodnoty</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PozadovanyZiadatelom" type="xs:boolean" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Príznak, či test bol požadovaný žiadateľom, alebo bol navyše indikovaný laboratóriom.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LaboratornaPolozka" type="IdPolozkaCiselnika" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Odkaz na číselník LaboratornaPolozka</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LaboratornePolozkyPreZapis">
    <xs:sequence>
      <xs:element name="LaboratornePolozky" type="IdPolozkaCiselnika" minOccurs="1" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Odkaz na číselník LaboratornaPolozka</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PlatnostOd" type="xs:dateTime" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="PlatnostDo" type="xs:dateTime" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="Laboratorium" type="IdOUPZS" minOccurs="1" maxOccurs="1">
